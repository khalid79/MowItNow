<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>InitialisationServlet</servlet-name>
		<servlet-class>fr.xebia.mowitnow.presentation.InitialisationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitialisationServlet</servlet-name>
		<url-pattern>/init</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PushPositionServlet</servlet-name>
		<servlet-class>fr.xebia.mowitnow.presentation.PushPositionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PushPositionServlet</servlet-name>
		<url-pattern>/pushPosition</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>